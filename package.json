{
  "name": "ember-cli-standard",
  "version": "0.1.0",
  "description": "Ember-cli standard support, for checking your application matches your coding standards.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "blueprints",
    "index.js"
  ],
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha node-tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arschmitz/ember-cli-standard.git"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "bugs": {
    "url": "https://github.com/arschmitz/ember-cli-standard/issues"
  },
  "homepage": "https://github.com/arschmitz/ember-cli-standard",
  "author": "Alex Schmitz",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "chai": "^3.5.0",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-blueprint-test-helpers": "^0.13.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^3.0.0",
    "ember-cli-release": "1.0.0-beta.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "es6-promise": "^4.0.0",
    "express": "^4.12.3",
    "fs-extra": "^0.30.0",
    "loader.js": "^4.0.1",
    "mocha": "^3.0.2",
    "testdouble": "^1.6.0"
  },
  "dependencies": {
    "broccoli-standard": "^0.1.0",
    "chalk": "^1.1.3",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-get-component-path-option": "^1.0.0",
    "ember-cli-get-dependency-depth": "^1.0.0",
    "ember-cli-is-package-missing": "^1.0.0",
    "ember-cli-lodash-subset": "^1.0.7",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-path-utils": "^1.0.0",
    "ember-cli-string-utils": "^1.0.0",
    "ember-cli-test-info": "^1.0.0",
    "ember-cli-valid-component-name": "^1.0.0",
    "ember-router-generator": "^1.0.0",
    "exists-sync": "0.0.3",
    "fs-extra": "^0.24.0",
    "inflection": "^1.7.1",
    "silent-error": "^1.0.0",
    "js-string-escape": "^1.0.0",
    "rsvp": "^3.2.1",
    "walk-sync": "^0.3.0"
  },
  "keywords": [
    "ember-addon",
    "lint",
    "eslint",
    "eslint ember",
    "eslint ember-cli",
    "standard",
    "code quality",
    "code formatting",
    "ember standard",
    "standard ember-cli",
    "ember"
  ],
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-standard"
  },
  "standard": {
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "afterEach",
      "visit",
      "andThen",
      "currentURL"
    ]
  }
}
